<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>HelloServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/servlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>FirstPage</servlet-name>
    <jsp-file>/FirstPage.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>FirstPage</servlet-name>
    <url-pattern>/fp</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SetCookiesServlet</servlet-name>
    <servlet-class>SetCookiesServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SetCookiesServlet</servlet-name>
    <url-pattern>/setservlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>GetCookiesServlet</servlet-name>
    <servlet-class>GetCookiesServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>GetCookiesServlet</servlet-name>
    <url-pattern>/getservlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DeleteCookiesServlet</servlet-name>
    <servlet-class>DeleteCookiesServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>DeleteCookiesServlet</servlet-name>
    <url-pattern>/delservlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LibraryServlet</servlet-name>
    <servlet-class>LibraryServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LibraryServlet</servlet-name>
    <url-pattern>/library</url-pattern>
  </servlet-mapping>


</web-app>
